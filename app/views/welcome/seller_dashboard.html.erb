<br>

  <div class="jumbotron">
    <h1><b><center>DASHBOARD</center></b></h1>
    <ul class="nav nav-tabs">
      <li><a data-toggle="tab" href="#buyer">Buyer</a></li>
      <li><a data-toggle="tab" href="#seller">Seller</a></li>
      <li><a data-toggle="tab" href="#home">Home</a></li>
      <li><a data-toggle="tab" href="#appointment">Appointments</a></li>
      <li><a data-toggle="tab" href="#appointment">Offers</a></li>
      <li><a data-toggle="tab" href="#appointment">Advice</a></li>
    </ul>
  </div>

  <div class="tab-content">
    <div id="buyer" class="tab-pane fade in active">
      <h2> <center>Favorites</center> </h2>

  <% @favorites.each do  |f| %>
    <% p = f.property %>
      <div class="jumbotron">
        <%= render partial: 'layouts/buyer_property_view', locals: { p: p } %>
      </div>
  <% end %>
    </div>





    <div id="seller" class="tab-pane fade in active">
    <h2> <center>My Listings</center> </h2>
    <p><center>
       <%= link_to "Create Listing", new_property_path, class: 'btn btn-primary' %>
    </center></p>


 
  <% @properties.each do  |p| %>



      <div class="col-md-1">
        <h4> <%= link_to "Edit Listing", edit_property_path(p), class: 'btn btn-success' %></h4> 
      </div>
      <div class="jumbotron">
        <%= render partial: "layouts/property", locals: { p: p } %>    
      </div>  

  <% end %>
    </div>
  </div>


<script>
$(document).ready(function(){
    $(".nav-tabs a").click(function(){
        $(this).tab('show');
    });
});
</script>






