<br>

  <div class="jumbotron">
    <h1><b><center>SELLER DASHBOARD</center></b></h1>
    <p><center>
        <%= link_to "Create Listing", new_property_path, class: 'btn btn-primary' %>
    </center></p>
  </div>



  <h2> <center>My Listings</center> </h2>
  <% @properties.each do  |p| %>
    <div class="jumbotron">
      <%= render partial: "layouts/property", locals: { p: p } %>    
    </div>   
  <% end %>
 


  <h2> <center>Favorites</center> </h2>
  <% @favorites.each do  |f| %>
    <% p = f.property %>
      <div class="jumbotron">
        <%= render partial: 'layouts/buyer_property_view', locals: { p: p } %>
      </div>
  <% end %>


</div>




