
<div class="jumbotron">
<h2>DASHBOARD</h2>
</div>
  <h1><%= render partial: "layouts/property", locals: { address: @properties.first.map_address } %></h1>
  
  <p>Your Listed Properties</p>

  <p><%= link_to "Create Property", new_property_path, class: 'btn btn-primary' %></p>


<div class="row">
  <div class="col-md-6">
    <h2>  PROPERTIES  </h2>
    <% @properties.each do  |p| %>
      <div class="panel panel-default">
        <div class="panel-body">

         <h4> <%= link_to p.address, property_path(p) %></h4> 
          <%= p.city %>
          <%= p.state %>
          <%= p.zip %>

        </div>
      </div>
    <% end %>
  </div>


  <div class="col-md-6">
    <h2> APPOINTMENTS </h2>
      <% @appointments.each do |a| %> 
        <div class="panel panel-default">
          <div class="panel-body">

          <ol> <li><b> <%= a = link_to a.slot %> </b></li> </ol>

          </div>
        </div>
      <% end %>
  </div>

</div>
