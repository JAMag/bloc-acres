<% @title="Moverable | Zero Agent Fees" %>

<div class="wrapper">


    <div id="signup" class="marquee">
        <div class="stick col-md-4 pull-right">





            <div class="title">
              <h1><b>Zero. Agent. Fees.</b></h1>
            </div>

            <div>
                <h5>People in the market for real estate are wasting tens of thousands in real estate agent fees.</h5>
                <h4>Sign Up.</h4>
                <h5>Moverable saves you thousands.</h5>
              <img class="profile-img" src="https://lh5.googleusercontent.com/-b0-k99FZlyE/AAAAAAAAAAI/AAAAAAAAAAA/eu7opA4byxI/photo.jpg?sz=120" alt="">
              <form class="form-signin">
                <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
                  <%= devise_error_messages! %>

                  <div>
                    <% f.label :email %>
                    <%= f.email_field :email, placeholder: 'Email', class: "form-control" %>
                  </div>
                  <div>
                    <% f.label :password %>
                    <%= f.password_field :password, placeholder: 'Password', class: "form-control"%>
                  </div>
                  <br />
                  <div>
                    <% f.label :password_confirmation %><br />
                    <%= f.password_field :password_confirmation, placeholder: 'Confirm Password', class: "form-control" %>
                  </div>
                  <br />
                  <div id="welcome-submit">
                    <%= f.submit class: 'btn btn-lg btn-primary btn-block'  %>
                  </div>
                <% end %>
              </form>
            </div>

        </div>

      <div class="pull-left">
          <ul class="bmenu">
            <li><a href="#">For sale by owner does not mean by yourself</a></li>
            <li><a href="#">Buying and Showing Guides</a></li>
            <li><a href="#">Automated Marketing, Offers and negotiation</a></li>
            <li><a href="#">Huge Savings</a></li>
            <li><a href="#">Recommended Top Rated Pros</a></li>
            <li><a href="#">Automated showings</a></li>
            <li><a href="#">The simplest and most comprehensive way to buy and sell a home</a></li>
          </ul>
      </div>
    </div>


    <div id="welcomethird" class="marquee">

